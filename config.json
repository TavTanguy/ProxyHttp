{
  "name": "proxyHttp",
  "listen": {
    "ip": "0.0.0.0",
    "http": {
      "port": 80
    },
    "https": {
      "port": 443,
      "key": "./keys/privatekey.pem",
      "cert": "./keys/certificate.pem"
    }
  },
  "ifNoHost": {
    "code": 404,
    "headers": {
      "Content-Type": "text/html",
      "Content-Length": 326,
      "Server": "ProxyTav"
    },
    "file": "./noHost.html"
  },
  "hosts": [
    {
      "defaultHost": false,
      "on": ["localhost", "127.0.0.1"],
      "redirect": {
        "name": "Server local",
        "ip": "127.0.80.1",
        "port": 8080
      }
    }
  ],
  "log": {
    "error": "./logs/error.log",
    "info": "./logs/info.log"
  }
}
