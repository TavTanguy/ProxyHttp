{
  "name": "proxyHttp",
  "listen": {
    "ip": "0.0.0.0",
    "http": {
      "port": 80
    },
    "https": {
      "port": 443,
      "key": "./keys/privatekey.pem",
      "cert": "./keys/certificate.pem"
    }
  },
  "hosts": [
    {
      "defaultHost": true,
      "on": ["localhost", "127.0.0.1"],
      "redirect": {
        "name": "Server local",
        "ip": "127.0.80.1",
        "port": 8080
      }
    }
  ]
}
